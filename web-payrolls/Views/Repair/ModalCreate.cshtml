<!--Form Create-->
<div class="modal fade" id="modal-add" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content" style="border-top: #28A745 3px solid;">
      <div class="modal-header">

        <h2 class="modal-title text-primary card-text" style="font-weight:600" id="exampleModalLabel">
          <i class="ti-plus pr-2"></i> Set Price
        </h2>

        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>

      <form method="post" id="form-set-price">
        <div class="modal-body">

          <input type="hidden" id="PK_Grade_Id" name="PK_Grade_Id" />

          @Html.AntiForgeryToken()
          <div class="row">

            <div class="form-group col-md-12">
              <label for="qty">Get Price</label>
              <input class="form-control" id="Get_Price" autocomplete="off" name="Get_Price" placeholder="Enter Get Price" maxlength="50" value="0" required=""/>
            </div>


            <div class="form-group col-md-12">
              <label for="qty">Other Spend</label>
              <input class="form-control" id="Other_Spend" autocomplete="off" name="Other_Spend" placeholder="Enter Other Spend" maxlength="50" value="0" required=""/>
            </div>

          </div>

        </div>


        <div class="modal-footer">
          <button type="submit" class="btn btn-block btn-sm" id="btn-save-more-grade" style="background: #28a745; color: #fff; font-size: 20px">
            <i class="ti-save-alt pr-2"></i>@Resources.Voucher.Save Close
          </button>
        </div>

      </form>

    </div>
  </div>
</div>
<!--Form Create-->
